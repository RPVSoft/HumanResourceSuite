
@{
    ViewData["Title"] = "Self Details";
    Layout = "~/Views/Shared/_LayoutEmployeeMaster.cshtml";

}


<body>

    <main id="main">
        <!-- Bread crumb and right sidebar toggle -->
        <!-- ============================================================== -->
        <div class="page-breadcrumb bg-white">
            <div class="row align-items-center">
                <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
                    <h4 class="page-title">Dashboard</h4>
                </div>
                <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
                    <div class="d-md-flex">
                        <ol class="breadcrumb ms-auto">
                            <li><a href="#" class="fw-normal">Bank Details</a></li>
                        </ol>
                        <a href="" target="_blank"
                           class="btn btn-danger  d-none d-md-block pull-right ms-3 hidden-xs hidden-sm waves-effect waves-light text-white">

                        </a>
                    </div>
                </div>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- ============================================================== -->
        <!-- End Bread crumb and right sidebar toggle -->
        <!-- ============================================================== -->

        <br />


        <!-- ============================================================== -->
        <!-- Container fluid  -->
        <!-- ============================================================== -->
        <div class="container-fluid">

            <div class="row">

                <!-- Column -->
                <div class="col-lg-12 col-xlg-12 col-md-12">
                    <div class="card">
                        <div class="card-body">
                            <form class="form-horizontal form-material">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-4  border-bottom p-0">
                                            <label class="p-0">Employee Code</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="12345" aria-label="First name">
                                        </div>
                                        <div class="col-md-4  border-bottom p-0">
                                            <label class="p-0">Photo URL</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="URL" aria-label="Last name">
                                        </div>

                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-4 border-bottom p-0">
                                            <label class="p-0">First Name</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="First Name" aria-label="First name">
                                        </div>
                                        <div class="col-md-4  border-bottom p-0">
                                            <label class="p-0">Middle Name</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="Middle Name" aria-label="Last name">
                                        </div>
                                        <div class="col-md-4  border-bottom p-0">
                                            <label class="p-0">Last Name</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="Last Name" aria-label="Last name">
                                        </div>
                                    </div>
                                </div>


                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-4 border-bottom p-0">
                                            <label class="p-0">Vendor ID</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="Vendor ID" aria-label="First name">
                                        </div>
                                        <div class="col-md-4  border-bottom p-0">
                                            <label class="p-0">Status</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="Status" aria-label="Last name">
                                        </div>
                                        <div class="col-md-4  border-bottom p-0">
                                            <label class="p-0">Pan Number</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="Pan Number" aria-label="Last name">
                                        </div>
                                    </div>
                                </div>


                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-4 border-bottom p-0">
                                            <label class="p-0">Gender</label>

                                            <select class="form-select shadow-none p-0 border-0 form-control-line">
                                                <option>Male</option>
                                                <option>Female</option>

                                            </select>
                                        </div>
                                        <div class="col-md-4  border-bottom p-0">
                                            <label class="p-0">Marital Status</label>

                                            <select class="form-select shadow-none p-0 border-0 form-control-line">
                                                <option>Un-Married</option>
                                                <option>Married</option>

                                            </select>
                                        </div>
                                        <div class="col-md-4  border-bottom p-0">
                                            <label class="p-0">DOB</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="DOB" aria-label="Last name">
                                        </div>
                                    </div>
                                </div>


                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-6 border-bottom p-0">
                                            <label class="p-0">Personal Email</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="Personal Email" aria-label="First name">
                                        </div>
                                        <div class="col-md-6  border-bottom p-0">
                                            <label class="p-0">Work Email</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="Work Email" aria-label="Last name">
                                        </div>

                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-4 border-bottom p-0">
                                            <label class="p-0">Extension</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="Extension" aria-label="First name">
                                        </div>
                                        <div class="col-md-4  border-bottom p-0">
                                            <label class="p-0">Work Phone</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="Work Phone" aria-label="Last name">
                                        </div>
                                        <div class="col-md-4  border-bottom p-0">
                                            <label class="p-0">Mobile Number</label>

                                            <input type="text" class="form-control  p-0 border-0" placeholder="Mobile Number" aria-label="Last name">
                                        </div>
                                    </div>
                                </div>

                             



                                <div class="form-group mb-4">
                                    <div class="col-sm-12">
                                        <button class="btn btn-success">Submit</button>
                                    </div>
                                </div>


                            </form>


                        </div>
                    </div>
                </div>
                <!-- Column -->

            </div>

        </div>
       
    </main>

    @section Scripts
{

    }
</body>









































